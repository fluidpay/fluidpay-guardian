        "@babel/core": "^7.17.8",
        "@babel/preset-env": "^7.16.11",
        "@types/node": "^17.0.23",
        "@typescript-eslint/eslint-plugin": "^5.18.0",
        "@typescript-eslint/parser": "^5.18.0",
        "eslint": "^8.12.0",
        "eslint-config-prettier": "^8.5.0",
        "path": "^0.12.7",
        "prettier": "^2.6.2",
        "typescript": "^4.6.3",
        "vite": "^2.9.1"
